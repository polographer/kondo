
<h1>Pictures</h1>

<table>
  <thead>
    <tr>
      <th>Thumb</th>
    </tr>
  </thead>

  <tbody>
    <% @pictures.each do |picture| %>
      <tr>
        <td><%= picture.web.attached? ? (image_tag picture.web.variant(resize_to_limit: [100, 100])) : "" %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Picture', new_picture_path %>
